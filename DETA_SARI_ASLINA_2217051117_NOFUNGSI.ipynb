      "source": [
        "banyak_bilangan = int(input('tuliskan jumlah bilangan: '))\n",
        "count = 0\n",
        "A1 = 0\n",
        "A2 = 1\n",
        " \n",
        "list_fibonacci = []\n",
        "while count < banyak_bilangan:\n",
        "   list_fibonacci.append(A1)\n",
        "   bilangan_terakhir = A1 + A2\n",
        "   A1 = A2\n",
        "   A2 = bilangan_terakhir\n",
        "   count += 1\n",
        "   \n",
        "print(list_fibonacci)"
      ]
